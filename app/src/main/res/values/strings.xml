    <string-array name="check_updates">
        <item>dnf check-update -q | awk \'NF\' | wc -l</item>                                                                                               <!--Fedora-->
        <item>zypper list-updates | awk \'BEGIN{ found=0} /----/{found=1}  {if (found) print }\' | tail -n +2 | grep -v \'/var/mail/root\' | wc -l</item>   <!--Suse-->
        <item>pacman -Qu | wc -l</item>                                                                                                                     <!--Arch-->
        <item>yum check-update -q | awk \'NF\' | wc -l</item>                                                                                               <!--Centos/redhat-->
        <item>(apt update 2>&amp;1 | grep -v ^$ | grep -v ^WARNING) 2>&amp;1>/dev/null &amp;&amp; apt list --upgradable 2>&amp;1 | grep -v ^$ | grep -v ^WARNING | grep -v Listing | wc -l | tail -n1</item>                                                                          <!--Ubuntu-->
        <item>(apt update 2>&amp;1 | grep -v ^$ | grep -v ^WARNING) 2>&amp;1>/dev/null &amp;&amp; apt list --upgradable 2>&amp;1 | grep -v ^$ | grep -v ^WARNING | grep -v Listing | wc -l | tail -n1</item>                                                                          <!--Debian-->
    </string-array>

    <string-array name="update_commands">
        <item>dnf -y update</item>          <!--Fedora-->
        <item>zypper update -y</item>       <!--Suse-->
        <item>pacman -Syu --noconfirm</item><!--Arch-->
        <item>yum -y update</item>          <!--Centos/redhat-->
        <item>apt -y upgrade</item>         <!--Ubuntu-->
        <item>apt -y upgrade</item>         <!--Debian-->
    </string-array>


